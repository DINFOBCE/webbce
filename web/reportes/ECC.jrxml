dbo.fneSaldoAnterior_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneDsctoNP(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
+ dbo.fneExceso(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneMora(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
+ dbo.fneSaldoMes_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS MontoCuota,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 0) AS CodCreditoPeriodo_0,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 1) AS CodCreditoPeriodo_1,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 2) AS CodCreditoPeriodo_2,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 3) AS CodCreditoPeriodo_3,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 4) AS CodCreditoPeriodo_4,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 5) AS CodCreditoPeriodo_5,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 6) AS CodCreditoPeriodo_6,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 7) AS CodCreditoPeriodo_7,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 8) AS CodCreditoPeriodo_8,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 9) AS CodCreditoPeriodo_9,
dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 10) AS CodCreditoPeriodo_10,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 0, @IdAnexo) AS DescuentoEfectuado_0,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 1, @IdAnexo) AS DescuentoEfectuado_1,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 2, @IdAnexo) AS DescuentoEfectuado_2,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 3, @IdAnexo) AS DescuentoEfectuado_3,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 4, @IdAnexo) AS DescuentoEfectuado_4,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 5, @IdAnexo) AS DescuentoEfectuado_5,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 6, @IdAnexo) AS DescuentoEfectuado_6,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 7, @IdAnexo) AS DescuentoEfectuado_7,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 8, @IdAnexo) AS DescuentoEfectuado_8,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 9, @IdAnexo) AS DescuentoEfectuado_9,
dbo.fneAnexoDescuento_DescuentoEfectuado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 10, @IdAnexo) AS DescuentoEfectuado_10,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 0, @IdAnexo) AS Exceso_0,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 1, @IdAnexo) AS Exceso_1,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 2, @IdAnexo) AS Exceso_2,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 3, @IdAnexo) AS Exceso_3,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 4, @IdAnexo) AS Exceso_4,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 5, @IdAnexo) AS Exceso_5,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 6, @IdAnexo) AS Exceso_6,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 7, @IdAnexo) AS Exceso_7,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 8, @IdAnexo) AS Exceso_8,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 9, @IdAnexo) AS Exceso_9,
dbo.fneAnexoDescuento_Exceso(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 10, @IdAnexo) AS Exceso_10,
-----03-------------

	dbo.fneAnexoPagoProyectado_0(@IdAnexo, GETDATE())  AS Proyectado_0,
	CASE WHEN dbo.fneSaldoAnterior_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	+ dbo.fneDsctoNP(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	+ dbo.fneExceso(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	+ dbo.fneMora(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	+ dbo.fneSaldoMes_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) =0 THEN 0 ELSE dbo.fneSaldoAnterior_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	+ dbo.fneDsctoNP(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	+  CASE WHEN dbo.fneConExcesoMora (@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa) ) <>0 THEN 0 ELSE dbo.fneExceso(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) END + CASE WHEN
	dbo.fneConExcesoMora (@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa) ) <>0 THEN 0  ELSE dbo.fneMora(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))
	END  + dbo.fneSaldoMes_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))END AS Proyectado,
	dbo.fneAnexoPagoProyectado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 1, @IdAnexo) AS Proyectado_1,
	dbo.fneAnexoPagoProyectado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 2, @IdAnexo) AS Proyectado_2,
	dbo.fneAnexoPagoProyectado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 3, @IdAnexo) AS Proyectado_3,
	dbo.fneAnexoPagoProyectado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 4, @IdAnexo) AS Proyectado_4,
	dbo.fneAnexoPagoProyectado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 5, @IdAnexo) AS Proyectado_5,
	dbo.fneAnexoPagoProyectado(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 6, @IdAnexo) AS Proyectado_6,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,0) ) AS CodCreditoPeriodo0,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-1) ) AS CodCreditoPeriodo,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-2) ) AS CodCreditoPeriodo1,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-3) ) AS CodCreditoPeriodo2,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-4) ) AS CodCreditoPeriodo3,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-5) ) AS CodCreditoPeriodo4,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-6) ) AS CodCreditoPeriodo5,
	dbo.fneAnexoDescuento_CodCreditoPeriodo(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), dbo.fneAnexo_NroMes (@IdAnexo,-7) ) AS CodCreditoPeriodo6

FROM   vAnexo_CodyNom (NOLOCK) INNER JOIN
                      dbo.Grado (NOLOCK) ON vAnexo_CodyNom.IdGrado = dbo.Grado.IdGrado INNER JOIN
                      dbo.Situacion (NOLOCK) ON vAnexo_CodyNom.IdSituacion = dbo.Situacion.IdSituacion INNER JOIN
                      dbo.Ubigeo (NOLOCK) ON vAnexo_CodyNom.IdUbigeo = dbo.Ubigeo.IdUbigeo INNER JOIN
                      dbo.EntidadDescuentoTD (NOLOCK)ON vAnexo_CodyNom.IdEntidadDescuento = dbo.EntidadDescuentoTD.IdEntidadDescuento CROSS JOIN
                      dbo.CreditoPeriodo (NOLOCK)
WHERE     (vAnexo_CodyNom.IdAnexo = @IdAnexo) AND (dbo.CreditoPeriodo.IdCreditoPeriodo = dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))]]>
	</queryString>

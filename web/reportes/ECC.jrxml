SET @Idanexo = (select dbo.anexo.idanexo from anexo where dbo.anexo.codanexo = @codanexo)
SET @Idempresa = 1
SELECT
----------------01------------------------
dbo.CreditoPeriodo.NomCreditoPeriodo,dbo.CreditoPeriodo.FechaFin, dbo.CreditoPeriodo.FechaInicio,vAnexo_CodyNom.CodAnexo,
vAnexo_CodyNom.Documento,dbo.Situacion.NomSituacion,vAnexo_CodyNom.NomAnexo, vAnexo_CodyNom.Quinquenio,
vAnexo_CodyNom.Direccion + N' ' + CASE WHEN vAnexo_CodyNom.IdUbigeo = 0 THEN '' ELSE NomUbigeo END AS Direccion,dbo.Grado.CodGrado,vAnexo_CodyNom.Telefono1,
dbo.EntidadDescuentoTD.CodEntidadDescuento,vAnexo_CodyNom.Percibo, vAnexo_CodyNom.CreditoOtorgado,vAnexo_CodyNom.CreditoAdicional,vAnexo_CodyNom.CreditoDisminucion,
vAnexo_CodyNom.CreditoOtorgadoTotal, vAnexo_CodyNom.CreditoAdicionalTotal,vAnexo_CodyNom.CreditoDisminucionTotal,vAnexo_CodyNom.FechaFinContrato,
dbo.fnEstadoPagare(@IdAnexo) AS EstadoPagare,	dbo.fneAnexo_Estado(@IdAnexo) AS NomEstado,
------ 02----
dbo.fneSaldoAnterior_Importe(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa))AS Importe_Anterior,
dbo.fneDsctoNP(@IdAnexo, @IdEmpresa,dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS DsctoNP_Anterior,
dbo.fneExceso(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS Exceso_Anterior,
dbo.fneMora(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS Mora_Anterior,
dbo.fneSaldoMes_Importe(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS Importe_Mes,
dbo.fneSaldoAFavor(@IdAnexo, @IdEmpresa,dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS SaldoFavor_Anterior,
dbo.fneSaldoAnterior_Importe(@IdAnexo, @IdEmpresa,
dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneDsctoNP(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneExceso(@IdAnexo, @IdEmpresa,
dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneMora(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneSaldoMes_Importe(@IdAnexo, @IdEmpresa,
dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) + dbo.fneSaldoAFavor(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS Total,
dbo.fneSeparacionRosada_MontoCuota(@IdAnexo,1, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS MontoCuotaRosada,
dbo.fneSeparacionBanca_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(),1)) AS MontoCuotaBanca,dbo.fneCreditoDisponible_Anexo(@IdAnexo) AS CreditoDisponible,
dbo.fneCreditoDisponibleTotal_Anexo(@IdAnexo) AS CreditoDisponibleTotal,dbo.fneAnexoDescuento_FechaFin(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 0) AS Fecha,
dbo.fneAnexoDescuento_DescuentoRemitido(dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa), 0, @IdAnexo) AS DescuentoRemitido,
dbo.fneSaldoAnterior_MontoCuota(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS MontoCuota_Anterior,
dbo.fneDsctoNP(@IdAnexo, @IdEmpresa, dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS DsctoNP_Mes,dbo.fneExceso(@IdAnexo, @IdEmpresa,dbo.fneCreditoPeriodo(GETDATE(), @IdEmpresa)) AS Exceso_Mes,
